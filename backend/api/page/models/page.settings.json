{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "name": "Page",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "slug": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "link-components.seo",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "blocks": {
      "type": "dynamiczone",
      "components": [
        "blocks.hero"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "publish_at": {
      "type": "datetime",
      "private": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    }
  }
}
